package com.lightning.bean.wuminghui;

public class Car {
	private int car_id;//运输车辆编号 不是null 主键
	private String car_code;//车牌号 不是null
	private int car_type;//车型 不是null
	private int car_size;//车辆载重容量 不是null
	private int car_state;//车辆状态 不是null
	private int car_belong_station;//出发网点id
	private int car_arrive_station;//到达网点id
	public int getCar_id() {
		return car_id;
	}
	public void setCar_id(int car_id) {
		this.car_id = car_id;
	}
	public String getCar_code() {
		return car_code;
	}
	public void setCar_code(String car_code) {
		this.car_code = car_code;
	}
	public int getCar_type() {
		return car_type;
	}
	public void setCar_type(int car_type) {
		this.car_type = car_type;
	}
	public int getCar_size() {
		return car_size;
	}
	public void setCar_size(int car_size) {
		this.car_size = car_size;
	}
	public int getCar_state() {
		return car_state;
	}
	public void setCar_state(int car_state) {
		this.car_state = car_state;
	}
	public int getCar_belong_station() {
		return car_belong_station;
	}
	public void setCar_belong_station(int car_belong_station) {
		this.car_belong_station = car_belong_station;
	}
	public int getCar_arrive_station() {
		return car_arrive_station;
	}
	public void setCar_arrive_station(int car_arrive_station) {
		this.car_arrive_station = car_arrive_station;
	}
	public Car(int car_id, String car_code, int car_type, int car_size, int car_state, int car_belong_station,
			int car_arrive_station) {
		super();
		this.car_id = car_id;
		this.car_code = car_code;
		this.car_type = car_type;
		this.car_size = car_size;
		this.car_state = car_state;
		this.car_belong_station = car_belong_station;
		this.car_arrive_station = car_arrive_station;
	}
	public Car() {
		super();
		// TODO Auto-generated constructor stub
	}
	
}