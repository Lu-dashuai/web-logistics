package com.lightning.bean.yuhaiyang;

public class Warehose {
	private int warehose_id;//仓库ID
	private int warehose_type;//仓库所装物品类型ID
	private int warehose_station;//所属网点ID
	private int warehose_size;//仓库容量
	
	public Warehose() {
		super();
	}
	public Warehose(int warehose_type, int warehose_station, int warehose_size) {
		super();
		this.warehose_type = warehose_type;
		this.warehose_station = warehose_station;
		this.warehose_size = warehose_size;
	}
	public Warehose(int warehose_id, int warehose_type, int warehose_station,
			int warehose_size) {
		super();
		this.warehose_id = warehose_id;
		this.warehose_type = warehose_type;
		this.warehose_station = warehose_station;
		this.warehose_size = warehose_size;
	}
	public int getWarehose_id() {
		return warehose_id;
	}
	public void setWarehose_id(int warehose_id) {
		this.warehose_id = warehose_id;
	}
	public int getWarehose_type() {
		return warehose_type;
	}
	public void setWarehose_type(int warehose_type) {
		this.warehose_type = warehose_type;
	}
	public int getWarehose_station() {
		return warehose_station;
	}
	public void setWarehose_station(int warehose_station) {
		this.warehose_station = warehose_station;
	}
	public int getWarehose_size() {
		return warehose_size;
	}
	public void setWarehose_size(int warehose_size) {
		this.warehose_size = warehose_size;
	}
	
}
